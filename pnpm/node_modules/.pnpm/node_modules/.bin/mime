#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/gmaisse/Documents/4.OpenSource/npm-yarn-node_modules/pnpm/node_modules/.pnpm/registry.npmjs.org/mime@1.6.0/node_modules/mime/node_modules:/Users/gmaisse/Documents/4.OpenSource/npm-yarn-node_modules/pnpm/node_modules/.pnpm/registry.npmjs.org/mime@1.6.0/node_modules:/Users/gmaisse/Documents/4.OpenSource/npm-yarn-node_modules/pnpm/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/gmaisse/Documents/4.OpenSource/npm-yarn-node_modules/pnpm/node_modules/.pnpm/node_modules:/Users/gmaisse/Documents/4.OpenSource/npm-yarn-node_modules/pnpm/node_modules:/Users/gmaisse/Documents/4.OpenSource/npm-yarn-node_modules/node_modules:/Users/gmaisse/Documents/4.OpenSource/node_modules:/Users/gmaisse/Documents/node_modules:/Users/gmaisse/node_modules:/Users/node_modules:/node_modules:/Users/gmaisse/Documents/4.OpenSource/npm-yarn-node_modules/pnpm/node_modules/.pnpm/node_modules/mime/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
